{
  "appId": "com.roqa.app",
  "appName": "Roqa",
  "webDir": "dist_web",
  "bundledWebRuntime": false,
  "server": {
    "url": "https://raqqa-v6cd.vercel.app",
    "androidScheme": "https",
    "cleartext": true,
    "allowNavigation": [
      "raqqa-v6cd.vercel.app",
      "*.vercel.app",
      "*.openai.com",
      "*.anthropic.com"
    ]
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": false
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    }
  }
}
